{
  "name": "discord_bot_dai_js",
  "version": "1.0.0",
  "description": "Bot de la comunidad de discord de la Escuela de Industriales de la UVigo, encargado de la gestión de usuarios, actividades, canales, votaciones, etc.",
  "main": "dist/index.js",
  "type": "module",
  "dependencies": {
    "colors": "^1.4.0",
    "discord-api-types": "0.37.119",
    "discord.js": "^14.19.1",
    "fast-deep-equal": "3.1.3",
    "glob": "^11.0.1",
    "lodash": "4.17.21",
    "lodash.snakecase": "4.1.1",
    "magic-bytes.js": "1.10.0",
    "node-bin-setup": "1.1.3",
    "pdfkit": "^0.16.0",
    "ts-mixer": "6.0.4",
    "tslib": "2.8.1",
    "undici": "7.3.0",
    "undici-types": "7.3.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "standard",
      "plugin:@typescript-eslint/recommended",
      "plugin:node/recommended"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "env": {
      "node": true,
      "es2021": true
    },
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": 2
    }
  },
  "scripts": {
    "start": "ts-node index.ts",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint 'src/**/*.{ts,js}' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cocotterooo/Discord_Bot_DAI_JS.git"
  },
  "keywords": [
    "dai",
    "eei",
    "uvigo",
    "cocoterooo",
    "m_medina33",
    "nodejs",
    "node"
  ],
  "author": "Manuel Medina Rodríguez",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Cocotterooo/Discord_Bot_DAI_JS/issues"
  },
  "homepage": "https://github.com/Cocotterooo/Discord_Bot_DAI_JS#readme"
}